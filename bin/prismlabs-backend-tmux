#!/bin/bash

source ~/.tmux/tmux-utils.sh

create_default_session ~/code/uptech/clients/prismlabs "prismlabs-backend" 0
create_window_at ~/code/uptech/clients/prismlabs/tempo-hosted-api "prismlabs-backend" "avatar-api-code" "zsh"

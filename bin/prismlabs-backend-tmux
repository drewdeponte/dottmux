create_window_at ~/code/uptech/clients/prismlabs/prism-api "prismlabs-backend" "prism-api-code" "zsh"
create_window_at ~/code/uptech/clients/prismlabs/prism-api "prismlabs-backend" "prism-api-build" "zsh"
create_window_at ~/code/uptech/clients/prismlabs/partner-api "prismlabs-backend" "partner-api-code" "zsh"
create_window_at ~/code/uptech/clients/prismlabs/partner-api "prismlabs-backend" "partner-api-build" "zsh"
create_window_at ~/code/uptech/clients/prismlabs/partner-portal "prismlabs-backend" "partner-portal-code" "zsh"
create_window_at ~/code/uptech/clients/prismlabs/partner-portal "prismlabs-backend" "partner-portal-build" "zsh"
